{
  "name": "mletras-auth-backend",
  "version": "1.1.3",
  "description": "Production-ready authentication and metering system for MLetras lyrics app",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "tail": "wrangler tail",
    "db:create": "wrangler d1 create mletras-auth-db",
    "db:migrate": "wrangler d1 migrations apply mletras-auth-db",
    "db:studio": "wrangler d1 execute mletras-auth-db --command='SELECT * FROM users LIMIT 10;'"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jose": "^5.9.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "wrangler": "^4.38.0"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "auth",
    "metering",
    "lyrics",
    "mletras"
  ],
  "author": "Belicon Group",
  "license": "MIT"
}
